#!/bin/bash

function notifyme () {
	start=$(date +%s)
	"$@"
	notify-send "I'm Finished!" "\"$(echo $@)\" took $(($(date +%s) - start)) seconds to finish."
	paplay ~/.local/sndfiles/ding_ding.wav
}

notifyme python cpso.py --npart 100 --ndim 190 --constants 2.1 2.1 2.1 --alpha 0.8 --testsize 0.2 --xbounds -6.0 6.0 --vbounds -4.0 0.25 --wbounds 0.4 0.9 --time 150 --data data/data.csv --target data/target.csv --labels data/feature_labels.csv --expname "Mean Particle Velocity - 00" --author "Nathan Fox" --outpath 00_report_velocities/ 
